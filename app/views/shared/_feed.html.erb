<% if @feed_items.any? %>
  <ol class="microposts">
    <%= turbo_frame_tag "feed_items", src: static_pages_path(format: :turbo_stream), loading: :lazy %>
    <%#= render @feed_items %>
    <% if @feed_items.next_page %>
      <%= turbo_frame_tag "load_more", src: static_pages_path(page: @feed_items.next_page, format: :turbo_stream), loading: :lazy %>
    <% end %>
  </ol>
<% end %>
